<%- include('partials/head', {title: 'Login'}) %>
<%- include('partials/navbar') %>
<body>
    <form method="POST">
        <h3>Login Form</h3>
        
          <label for="username" class="form-label">Username:</label>
          <input type="text" class="form-control" id="username" aria-describedby="emailHelp" name="username" placeholder="Your Username" required><br><br>
        
        
          <label for="password" class="form-label">Password:</label>
          <input type="password" class="form-control" id="password" name="password" placeholder="Your Password" required><br><br>
        
        
            <span id="passwordHelpInline" class="form-text">
              Must be 6-12 characters long.
            </span><br><br>
        
    
        <button onclick="location.href='home'" class="btn btn-primary">Log In</button>
      </form>
</body>
</html>